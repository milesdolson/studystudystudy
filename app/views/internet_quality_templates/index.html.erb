<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All internet qualities
    </h1>

    <a href="/internet_qualities/new" class="btn btn-block btn-outline-secondary">
      Add a new internet quality
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Quality
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @internet_qualities.each do |internet_quality| %>
      <tr>
        <!-- Display internet_quality.id start -->
        <td>
          <%= internet_quality.id %>
        </td>
        <!-- Display internet_quality.id end -->

        <!-- Display internet_quality.quality start -->
        <td>
          <%= internet_quality.quality %>
        </td>
        <!-- Display internet_quality.quality end -->

        <!-- Display internet_quality.created_at start -->
        <td>
          <%= time_ago_in_words(internet_quality.created_at) %> ago
        </td>
        <!-- Display internet_quality.created_at end -->

        <!-- Display internet_quality.updated_at start -->
        <td>
          <%= time_ago_in_words(internet_quality.updated_at) %> ago
        </td>
        <!-- Display internet_quality.updated_at end -->

        <td>
          <a href="/internet_qualities/<%= internet_quality.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
